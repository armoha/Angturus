import init;
import bgmplayer as bgm;
import combat.flow as cf;
import txtManager as tM;
import chatEvents;
import changeBGM;
import etc.lightOn;
import etc.dayNight;
import NQCEvents;
import soundEffects;

function onPluginStart() {
    soundEffects.load();
    init.init();
    // GetStringIndex("Y");
}
function beforeTriggerExec() {
    init.pCheck();
    lightOn.main();
    dayNight.main();
    NQCEvents.main();
}
function afterTriggerExec() {
    chatEvents.QCcheck();
    changeBGM.main();
    bgm.checkbgm();
    cf.main();
    tM.main();
    SetMemory(0x6509A0, SetTo, 0);  // EUD 터보
}